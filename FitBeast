        _G.on = false
        loadstring(game:HttpGet('https://pastebin.com/raw/01jNR0Jt'))()
    end
end)

Section:NewToggle("AutoBuy Murder", "Buy Murder Every 45 Sec", function(state)
    if state then
        _G.on = true
        while _G.on and wait(45) do
            local args = {
            [1] = 60,
            [2] = {
                ["confirm"] = true,
                ["image"] = 0,
                ["name"] = "\nBe the murderer \n [Who did it]",
                ["price"] = 500,
                ["data"] = "requestmurderer"
            }
        }
        
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
        end
    else
        _G.on = false
        while _G.on and wait(45) do
            local args = {
            [1] = 60,
            [2] = {
                ["confirm"] = true,
                ["image"] = 0,
                ["name"] = "\nBe the murderer \n [Who did it]",
                ["price"] = 500,
                ["data"] = "requestmurderer"
            }
        }
        
        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
        end
    end
end)

